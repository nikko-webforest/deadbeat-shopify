<main class="db-main site-main db-page--media">
  
  {% section 'static-blog-listing' %}

</main>
<script>
  const DB_SF_Page_Media_Options = {
    name: 'DB SF Page Media Options',
    data() {
      return {}
    },
    methods: {
      loadPage() {
        console.log('loadPage');
        setTimeout(() => {
          document.querySelector('.db-site-loader').classList.remove('--show');
          // document.querySelector('.db-section--banner').classList.add('--animate-now');
          document.querySelector('.db-header').classList.add('--animate-now');
        }, 3000);
      },
    },
    computed: {},
    created() {
      this.loadPage();
    },
    mounted() {},
  };

  Vue.createApp(DB_SF_Page_Media_Options).mount('.db-page--media');
</script>