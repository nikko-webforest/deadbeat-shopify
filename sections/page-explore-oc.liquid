      onChangeUndeadCardSlide() {
        this.isActiveBox = !this.isActiveBox;
        const container = this.$refs.undeadSlider;
        
        const active = container.querySelector('.active-box');
        const notActive = container.querySelector('.nonactive-box');

        active.classList.remove(['active-box']);
