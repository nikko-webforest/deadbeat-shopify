        this.isVideoHover = true
        console.log(`Mouse Enter`);
      },
      handleMouseLeave() {
        this.isVideoHover = false
        console.log(`Mouse Leave`);
      },
      isFullScreen() {
        return !!(
          document.fullScreen || document.fullscreenElement || document.mozFullScreen || document.webkitIsFullScreen || document.msFullscreenElement
        )
      },
      handleFullScreen() {
        if (this.isFullScreen()) {
          if (document.exitFullscreen) document.exitFullscreen();
          else if (document.mozCancelFullScreen) document.mozCancelFullScreen();
          else if (document.webkitCancelFullScreen)
            document.webkitCancelFullScreen();
          else if (document.msExitFullscreen) document.msExitFullscreen();

          this.isVideoFullScreen = !this.isVideoFullScreen
        } else {
          const videoContainer = this.$refs.videoContainer;
          const video = this.$refs.video;

          if (videoContainer.requestFullscreen)
          videoContainer.requestFullscreen();
          else if (videoContainer.mozRequestFullScreen)
            videoContainer.mozRequestFullScreen();
          else if (videoContainer.webkitRequestFullScreen) {
            video.webkitRequestFullScreen();
          } else if (videoContainer.msRequestFullscreen)
            videoContainer.msRequestFullscreen();

          this.isVideoFullScreen = !this.isVideoFullScreen;
        }
      },
      formatDuration(time) {
        const leadingZeroFormatter = new Intl.NumberFormat(undefined, {
          minimumIntegerDigits: 2,
        })

        const seconds = Math.floor(time % 60);
        const minutes = Math.floor(time / 60) % 60;
        const hours = Math.floor(time / 3600);

        if (hours === 0) {
          return `${minutes}:${leadingZeroFormatter.format(seconds)}`
        } else {
          return `${hours}:${leadingZeroFormatter.format(minutes)}:${leadingZeroFormatter.format(seconds)}`;
        }
      },
      handleLoadedMetaData() {
        const progress = this.$refs.progress;
        const video = this.$refs.video;
        const totalTime = this.$refs.totalTime;

        progress.setAttribute('max', video.duration);
        totalTime.textContent = this.formatDuration(video.duration);
      },
      handleTimeUpdate() {
        const video = this.$refs.video;
        const progress = this.$refs.progress;
        const progressBar = this.$refs.progressBar;
        const currentTime = this.$refs.currentTime;

        if (!progress.getAttribute('max')) {
          progress.setAttribute('max', video.duration);
        }

        currentTime.textContent = this.formatDuration(video.currentTime);
        progress.value = video.currentTime;
        progressBar.style.width = Math.floor((video.currentTime / video.duration) * 100) + '%';
      },
      handleProgress(e) {
        const progress = this.$refs.progress;
        const video = this.$refs.video;

        const scrubTime = (e.offsetX / progress.offsetWidth) * video.duration;

        video.currentTime = scrubTime;
      },
      handleVideoClick() {
        const video = this.$refs.video;
        if (video.paused || video.ended) {
          video.play();
        } else {
          video.pause();
        }
      },
      handleVolume() {
        const video = this.$refs.video;
        video.muted = !video.muted;

        this.changeButtonState('mute');
      },
      changeButtonState(type) {
        const video = this.$refs.video;

        if (type === 'playpause') {
         if (video.paused || video.ended) {
          this.isActiveVideo = false;
         } else {
          this.isActiveVideo = true;
         }
        } else if (type === 'mute') {
          this.isMutedVideo = !this.isMutedVideo;
        }
      },
      handlePlay() {
        this.changeButtonState('playpause');
      },
      handlePause() {
        this.changeButtonState('playpause');
      },
      onChangeClickCardSlide(event) {
        const targetElement = event.target;

        const parentElement = targetElement.parentElement;

        if (parentElement.classList.contains('nonactive-box')) {
          this.onChangeUndeadCardSlide();
          return;
        }

        return;
