      async onChangeCardPosition() {
        return new Promise((resolve, reject) => {
          this.changeBgImage('next');
          const sliderContainer = this.$refs.newSliderContainer;
          const first = sliderContainer.querySelector('.--first');
          const second = sliderContainer.querySelector('.--second');
