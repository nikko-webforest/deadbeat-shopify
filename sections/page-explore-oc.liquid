    mounted() {
      const explore_oc_the_game = document.querySelector('#the-game');
      // const dbItemHoverIndicator = this.$refs.indicator;

      const compete = this.$refs.compete;
      const characterCreation = this.$refs.characterCreation;
      const breakthrough = this.$refs.breakthrough;
      const community = this.$refs.community;

      console.log(explore_oc_the_game);

      const revealSection = (entries, observer) => {
        const [entry] = entries;
        console.log(entry.isIntersecting);
        if (!entry.isIntersecting) {
          return;
        } else {
          setTimeout(() => {
            compete.classList.add('active')
          }, 1000);

          setTimeout(() => {
            compete.classList.remove(['active']);
            characterCreation.classList.add('active')
          }, 2000);

          setTimeout(() => {
            characterCreation.classList.remove(['active'])
            breakthrough.classList.add('active')
          }, 3000);

          setTimeout(() => {
            breakthrough.classList.remove(['active'])
            community.classList.add('active')
          }, 4000);

          setTimeout(() => {
            community.classList.remove(['active']);
          }, 5000);

          observer.unobserve(entry.target);
        }
      }

      const explore_oc_the_game_options = {
        root: null,
        threshold: 0.70,
      }

      const explore_oc_the_game_observer = new IntersectionObserver(revealSection, explore_oc_the_game_options);

      explore_oc_the_game_observer.observe(explore_oc_the_game);
