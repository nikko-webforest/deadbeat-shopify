          third.classList.remove(['--third']);
          lastElementSibling.classList.remove(['--hidden']);
          
          first.classList.add('--dissapearFirstElement');
          second.classList.add('--first');
          third.classList.add('--second');
          lastElementSibling.classList.add('--appearLastElement');

          setTimeout(() => {
            first.classList.remove(['--dissapearFirstElement']);
            sliderContainer.appendChild(first);
            first.classList.add('--hidden');
            lastElementSibling.classList.remove(['--appearLastElement']);
            lastElementSibling.classList.add('--third');
            resolve();
          }, 700);

          // setTimeout(() => {
