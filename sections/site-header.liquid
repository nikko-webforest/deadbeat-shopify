            <div class="db-btn-icon">
              <svg class="svg-icon-veggie-burger db-svg"
                width="45"
                height="13"
                viewBox="0 0 45 13"
                fill="none"
                xmlns="http://www.w3.org/2000/svg">
                <path id="path_01"
                  d="M1 1H44"
                  stroke="#FFFFFF"
                  stroke-width="2"
                  stroke-linecap="round">
                </path>
                <path id="path_02"
                  d="M1 12H44"
                  stroke="#FFFFFF"
                  stroke-width="2"
                  stroke-linecap="round">
                </path>
              </svg>
            </div>
          </div>
        </button>
        <a class="db-site-link"
          title="Deadbeat"
          href="/">
          <img class="db-site-logo db-img"
            alt="Deadbeat"
            title="Deadbeat"
            src="{{ 'DB_Main_logo-v02-1024x394.png' | asset_url }}"
            width=""
            height=""
            loading="lazy"/>
        </a>
