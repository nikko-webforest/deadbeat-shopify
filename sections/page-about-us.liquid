        },
        timeoutId:null,
        isMobile: false,
        windowSize: window.innerWidth,
        // addition
        isRight: true,
        isLeft: false,
      }
    },
    created() {
      if (window.innerWidth <= 768) {
        this.isMobile = true
      } else {
        this.isMobile = false
      }
      this.onLoadAnimation();
    },
    mounted() {
      const wrapper = this.$refs.wrapper;

      // Window Resize
      
      const handleWindowResize = () => {

        if (window.innerWidth > 768) {
          console.log(`Window Size`);
          this.isMobile = false;
          this.windowSize = window.innerWidth;
          this.isLeft = false;
          this.isRight = true;
        }

        if (window.innerWidth <= 768) {
          console.log(`I am in the lower window`);
          this.isMobile = true; 
        }
      }

      const debounce = (func, delay) => {
        let timeoutId;
        return function (...args) {
          clearTimeout(timeoutId);

          timeoutId = setTimeout(() => {
            func.apply(this, args);
          }, delay);
        };
      }
      const debouncedResizeHandler = debounce(handleWindowResize, 100);
      window.addEventListener('resize', debouncedResizeHandler);
      document.addEventListener("mousemove", this.dragging);
      document.addEventListener("mouseup", this.dragstop);
    },
    methods: {
      onChangeSlide() {
        console.log(`Click`);
        
      },
      dragStart(e) {
        const carousel = this.$refs.carousel;
        this.settingSlider.isDragStart = true;
        this.settingSlider.prevPageX = e.pageX || e.touches[0].pageX;
        this.settingSlider.prevScrollLeft = carousel.scrollLeft;
      },
      dragging(e) {
        if (!this.settingSlider.isDragStart) {
          return;
        }

        e.preventDefault();
        const carousel = this.$refs.carousel;
        this.settingSlider.isDragging = true;
        carousel.classList.add("dragging");
        this.settingSlider.positionDiff = (e.pageX || e.touches[0].pageX) - this.settingSlider.prevPageX;

        carousel.scrollLeft = this.settingSlider.prevScrollLeft - this.settingSlider.positionDiff;
      },
      dragstop() {
        const carousel = this.$refs.carousel;
        this.settingSlider.isDragStart = false;
        carousel.classList.remove("dragging");
        if (!this.settingSlider.isDragging) {
          return
        }
        this.settingSlider.isDragging = false;
        
      },
      onLoadAnimation() {
        console.log('onLoadAnimation');
        setTimeout(() => {
          document.querySelector('.db-site-loader').classList.remove('--show');
          // document.querySelector('.db-section--banner').classList.add('--animate-now');
          document.querySelector('.db-header').classList.add('--animate-now');
        }, 3000);
      },
      loopCharacterList() {
        let convertMembers = [...this.data.listOfCharacters];
        let members = [];

        for (person of convertMembers) {
          let nonReactiveCopy1 = Object.assign({}, person);
          let nonReactiveCopy2 = { ...nonReactiveCopy1 };
          let nonReactiveCopy3 = JSON.parse(JSON.stringify(nonReactiveCopy2));
          members.push(nonReactiveCopy3);
        }

        let position = {
          next: true,
          active: false,
          last: false,
        }

        const result = [];
        // Loop through the original array and create inner arrays
        for (let i = 0; i < members.length; i += 3) {
          const innerArray = members.slice(i, i + 3);
          result.push(innerArray);
        }
        

        return result;
			},
      selectActive(index) {
        let slideIndex = 0;
        let position = {
          next: true,
          active: false,
          last: false,
				}

        if (index === 0) {
          position = {
            ...position,
            next: false,
						active: true
          }

          return position
        }

        if (index === this.loopCharacterList().length - 1) {

          position = {
            ...position,
            next: false,
						last: true
          }

          return position
        }
				
        return position;
      },
      nativeProperty(property) {
        if (property === 'firstNative') {
          return {
            'firstNative': true
          }
        }

        if (property === 'secondNative') {
          return {
            'secondNative': true
          }
        }

        if (property === 'thirdNative') {
          return {
            'thirdNative': true
          }
        }
      },
      onChangeSlider(arrow) {
        const carousel = this.$refs.carousel;

        const firstImgWidth = carousel.querySelectorAll(".native-card")[0].clientWidth + 44;

        if (arrow === 'right') {
          this.inActiveButton = 'right';

          // Width
          carousel.scrollLeft = carousel.scrollLeft + firstImgWidth;

        }

        if (arrow === 'left') {
          this.inActiveButton = 'left';

          // Width
          carousel.scrollLeft = carousel.scrollLeft - firstImgWidth;
        }
      },
      onChangeMbSlider(type) {
        const sliderContainer = this.$refs.sliderContainer;

        if (type === 'left') {
          this.inActiveButton = 'left';
          activateSlider(sliderContainer, 'prev');
        }

        if (type === 'right') {
          this.inActiveButton = 'right';
          activateSlider(sliderContainer, 'next');
        }
      },
      onInfiniteScroll() {
        const carousel = this.$refs.carousel;
        
        let scrollWidth = carousel.scrollWidth - carousel.clientWidth;
        
        this.isLeft = carousel.scrollLeft == 0 ? false : true;
        this.isRight = carousel.scrollLeft == scrollWidth ? false : true;
        
        // Clear existing timeout & start autoplay if mouse is not hovering over carousel
      },
      onPreventDrag(event) {
        event.preventDefault();
      }
    },
    computed: {
